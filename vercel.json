{
  "version": 2,
  "builds": [
    {
      "src": "api/submit-form.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/api/submit-form",
      "dest": "/api/submit-form.js"
    }
  ]
}
